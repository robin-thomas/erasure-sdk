    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@erasure/abis": "^1.3.13",
    "@erasure/crypto-ipfs": "^1.2.4",
    "babel-loader": "^8.0.6",
